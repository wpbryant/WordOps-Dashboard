# WordOps Dashboard Configuration
# Generated by install.sh - do not edit manually

# Security
WO_DASHBOARD_SECRET_KEY={{SECRET_KEY}}

# CORS (configured for the dashboard domain)
WO_DASHBOARD_CORS_ORIGINS=["https://{{DOMAIN}}"]

# Authentication
WO_DASHBOARD_ADMIN_USERNAME={{ADMIN_USERNAME}}
WO_DASHBOARD_ADMIN_PASSWORD_HASH={{ADMIN_PASSWORD_HASH}}

# Netdata integration (optional)
WO_DASHBOARD_NETDATA_URL=http://127.0.0.1:19999

# Debug mode (disable in production)
WO_DASHBOARD_DEBUG=false
